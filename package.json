    "gulp": "4.0.2",
    "lint-staged": "^12.4.0",
    "prettier": "^2.7.1",
    "rimraf": "3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "4.9.3",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-glslify": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.1",
